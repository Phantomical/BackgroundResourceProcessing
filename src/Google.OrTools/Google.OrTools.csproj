<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <LangVersion>12</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Google.OrTools.runtime.win-x64"   Version="9.12.4544" />
    <PackageReference Include="Google.OrTools.runtime.osx-x64"   Version="9.12.4544" />
    <PackageReference Include="Google.OrTools.runtime.osx-arm64" Version="9.12.4544" />
    <PackageReference Include="Google.OrTools.runtime.linux-x64" Version="9.12.4544" />
  </ItemGroup>
  <PropertyGroup>
    <!--
      These warnings are normally useful but don't apply for the code here.

      Also it's partly autogenerated code copied from the OrTools project.
    -->
    <NoWarn>$(NoWarn);CS0660;CS0661</NoWarn>
  </PropertyGroup>
  <Target Name="PostClean" AfterTargets="Clean">
    <RemoveDir Directories="$(ProjectDir)\bin" />
  </Target>
</Project>
