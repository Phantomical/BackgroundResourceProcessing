<?xml version="1.0" encoding="UTF-8"?>

<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <!--
        Don't modify this file. It is meant purely to allow the
        UnifiedBackgroundProcessing project to be built directly, without having
        to build at the solution level (which disallows specifying the runtime).
    -->
    <Import
        Condition=" '$(SolutionDir)' == '' And Exists('$(MSBuildProjectDirectory)\..\..\UnifiedBackgroundProcessing.props.user') "
        Project="$(MSBuildProjectDirectory)\..\..\UnifiedBackgroundProcessing.props.user"
    />    
</Project>
